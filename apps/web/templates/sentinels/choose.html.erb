<div class="jumbotron">
  <h1>Choose your Sentinel</h1>
  <p>
  <form class="form-inline">
    <div class="form-group">
      <label class="sr-only" for="Sentinel">Name</label>
      <select class="form-control" name="sentinel">
        <% sentinels.each do |sentinel| %>
          <option value="<%= sentinel %>"><%= sentinel %></option>
        <% end %>
      </select>
    </div>
    <button type="submit" class="btn btn-primary">View Sentinel</button>
  </form>
  </p>
</div>